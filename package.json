    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "rollup-plugin-vue": "^6.0.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "vue": "^3.0.5",
    "@vue/compiler-sfc": "^3.0.0",
